<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fitem"
    android:orientation="vertical"
    tools:context="com.webmyne.constaint.FoodActivity">

    <android.support.v7.widget.CardView
        android:id="@+id/button8"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerInParent="true"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="180dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="180dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="55dp"
        android:adjustViewBounds="true"
        android:background="@color/white"
        android:clickable="true"
        android:rotation="45"
        android:visibility="visible"
        app:cardCornerRadius="@dimen/_4sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button9"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical"
            android:rotation="315">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/_45sdp"
                android:layout_gravity="center"
                android:src="@drawable/ic_dinner"
                tools:ignore="ContentDescription" />

            <com.webmyne.constaint.custom.TfTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Dinner"
                android:textColor="@color/black"
                android:textSize="@dimen/_12ssp"
                app:ftype="medium"
                tools:ignore="HardcodedText" />

        </LinearLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/button11"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerInParent="true"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="110dp"
        android:layout_marginLeft="110dp"
        android:layout_marginRight="110dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="55dp"
        android:adjustViewBounds="true"
        android:background="@color/white"
        android:clickable="true"
        android:rotation="45"
        android:visibility="visible"
        app:cardCornerRadius="@dimen/_4sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button10"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical"
            android:rotation="315">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/_45sdp"
                android:layout_gravity="center"
                android:src="@drawable/ic_desserts"
                tools:ignore="ContentDescription" />

            <com.webmyne.constaint.custom.TfTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Desserts"
                android:textColor="@color/black"
                android:textSize="@dimen/_12ssp"
                app:ftype="medium"
                tools:ignore="HardcodedText" />

        </LinearLayout>

    </android.support.v7.widget.CardView>

    <eightbitlab.com.blurview.BlurView
        android:id="@+id/blurView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:blurOverlayColor="@color/transparent"
        tools:ignore="MissingConstraints"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="0dp">

        <!--Any child View here, TabLayout for example-->
        <com.webmyne.constaint.custom.TfEditText
            android:id="@+id/edtSearch"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:background="@drawable/blur"
            android:drawableRight="@drawable/ic_search"
            android:drawableTint="@color/white"
            android:hint="Search"
            android:inputType="textVisiblePassword"
            android:padding="@dimen/_8sdp"
            android:textColorHint="@color/white"
            android:textSize="@dimen/_14ssp"
            tools:ignore="HardcodedText,MissingConstraints,RtlHardcoded,UnusedAttribute"
            tools:layout_editor_absoluteY="8dp"
            tools:layout_editor_absoluteX="8dp" />
    </eightbitlab.com.blurview.BlurView>

    <android.support.v7.widget.CardView
        android:id="@+id/button9"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerInParent="true"
        android:layout_marginEnd="180dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="180dp"
        android:layout_marginStart="32dp"
        android:adjustViewBounds="true"
        android:background="@color/white"
        android:clickable="true"
        android:rotation="45"
        android:visibility="visible"
        app:cardCornerRadius="@dimen/_4sdp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="110dp"
        app:layout_constraintTop_toBottomOf="@+id/blurView">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical"
            android:rotation="315">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/_45sdp"
                android:layout_gravity="center"
                android:src="@drawable/ic_breakfast"
                tools:ignore="ContentDescription" />

            <com.webmyne.constaint.custom.TfTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Break Fast"
                android:textColor="@color/black"
                android:textSize="@dimen/_12ssp"
                app:ftype="medium"
                tools:ignore="HardcodedText" />

        </LinearLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/button10"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerInParent="true"
        android:layout_marginEnd="110dp"
        android:layout_marginLeft="110dp"
        android:layout_marginRight="110dp"
        android:layout_marginStart="110dp"
        android:adjustViewBounds="true"
        android:background="@color/white"
        android:clickable="true"
        android:rotation="45"
        android:visibility="visible"
        app:cardCornerRadius="@dimen/_4sdp"
        app:layout_constraintBottom_toTopOf="@+id/button8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button9"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical"
            android:rotation="315">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/_45sdp"
                android:layout_gravity="center"
                android:src="@drawable/ic_lunch"
                tools:ignore="ContentDescription" />

            <com.webmyne.constaint.custom.TfTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Lunch"
                android:textColor="@color/black"
                android:textSize="@dimen/_12ssp"
                app:ftype="medium"
                tools:ignore="HardcodedText" />

        </LinearLayout>
    </android.support.v7.widget.CardView>

</android.support.constraint.ConstraintLayout>

